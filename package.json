{
  "name": "tickets-realtime",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:dashboard": "vite --config vite.dashboard.config.ts",
    "dev:unified": "vite --config vite.unified.config.ts",
